<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- -->

<hibernate-mapping>
	<class name="entity.Users" table="USERS">
		<id name="uid" type="int">
			<generator class="native"></generator>
		</id>
		<property name="role" type="java.lang.String"></property>
		<property name="username" type="java.lang.String"/>
		<property name="password" type="java.lang.String"/>
		<property name="name" type="java.lang.String"></property>
		<property name="gender" type="java.lang.String"/>
		<property name="birthday" type="date"/>
		<property name="position" type="java.lang.String"></property>
		<property name="onboardT" type="date"/>
		<property name="phone" type="java.lang.String"/>
		<property name="email" type="java.lang.String"></property>
		<property name="qq" type="java.lang.String"/>
		<property name="weibo" type="java.lang.String"/>
		<property name="photo" type="java.lang.String"></property>
		<property name="weixin" type="java.lang.String"/>
		<property name="mypage" type="java.lang.String"/>
	</class>
</hibernate-mapping>
